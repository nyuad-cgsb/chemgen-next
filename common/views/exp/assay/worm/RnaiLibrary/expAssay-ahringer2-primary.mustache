<div class="">
  <div class="row">
    <div class="col-md-6">
      <div id="screenInfo">
        <h2>Screen Information</h2>
        <p><strong>Screen</strong> {{{workflowData.screenName}}}</p>
        <p><strong>Gene</strong> {{{libraryData.cosmid_id}}} {{{wellData.annotationData.taxTerm}}}</p>
        <p><strong>Upload Name</strong> {{{workflowData.name}}}</p>
        <p><strong>Assay Date</strong>{{{workflowData.assayDate}}}</p>
        <P><strong>Imaging Date</strong>{{{expPlate.plateImageDate}}}</P>
        <h3>Plate View</h3>
        <!--TODO Make sure plateURL is correct Format-->
        <a href="{{{expPlate.plateWpPost}}}">{{expPlate.barcode}}</a>
      </div>
      <div id="libraryInfo">
        {{#wellData.parentLibraryData.rnaiId}}
          <h3> Parent Library Info </h3>
          <table class="table responsive">
            <tbody>
            <tr>
              <td>BioLoc</td>
              <td>{{wellData.parentLibraryData.bioloc}}</td>
            </tr>
            <tr>
              <td>Fwd Primer</td>
              <td>{{wellData.parentLibraryData.fwdPrimer}}</td>
            </tr>
            <tr>
              <td>Reverse Primer</td>
              <td>{{wellData.parentLibraryData.revPrimer}}</td>
            </tr>
            <tr>
              <td>Stock Title</td>
              <td>{{wellData.parentLibraryData.stocktitle}}</td>
            </tr>
            <tr>
              <td>Stock Location</td>
              <td>{{wellData.parentLibraryData.stockloc}}</td>
            </tr>
            </tbody>
          </table>
        {{/wellData.parentLibraryData.rnaiId}}
      </div>
    </div>
    <div class="col-md-6">
      <div id="taxTerms">
        <table class="table responsive">
          <thead>
          <tr>
            <th>Tag</th>
            <th>Value</th>
          </tr>
          </thead>
          <tbody>
          {{{table}}}
          </tbody>
        </table>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <section id="screenGeneGallery">
        <div>
          <!--Tax Term Header-->
          {{#hasTaxTerm}}
            <h2>{{{taxTerm}}}</h2>
          {{/hasTaxTerm}}
          <!-- !Tax Term Header-->

          <h3> {{{screenType}}} {{{workflowData.temperature}}}C Gene: {{{wellData.annotationData.taxTerm}}} {{{libraryData.cosmid_id}}} </h3>

          <!-- Permissive Mutant Strain (mel-28)+ L4440  ctrl_strain-->
          {{#annotationData.ctrl_strain}}
            <div id="enviraCPM">
              <h3>{{{screenType}}} {{{workflowData.temperature}}}C WS {{{ workflowData.biosamples.experimentBiosample.name }}} + L4440 </h3>
              <p>
                <!--This should always be on 1 line!-->
                [envira-gallery-dynamic id="tags-EGI-{{{annotationData.ctrl_strain.expGroupId}}}" columns="4" limit="4" sort="random"]
              </p>
            </div>
          {{/annotationData.ctrl_strain}}
          <!-- !Permissive Mutant Strain + L4440-->

          <!-- Permissive Mutant Strain (mel-28) + RNAi treat_rnai-->
          {{#hasTaxTerm}}
            <div id="enviraEM">
              <h3>{{{screenType}}} {{{workflowData.temperature}}}C WS {{{ workflowData.biosamples.experimentBiosample.name }}} + {{{libraryData.cosmid_id}}} {{{wellData.annotationData.taxTerm}}} RNAi </h3>
              <p>
                [envira-gallery-dynamic id="tags-EGI-{{{annotationData.treat_rnai.expGroupId}}}" columns="2"]
              </p>
            </div>
          {{/hasTaxTerm}}
          <!-- !Permissive Mutant Strain + RNAi-->

          <!-- Permissive WildType Strain (N2) + L4440 ctrl_null-->
          {{#annotationData.ctrl_null}}
            <div id="enviraCPN">
              <h3>{{{screenType}}} {{{workflowData.temperature}}}C WS {{{ workflowData.biosamples.ctrlBiosample.name }}} + L4440</h3>
              <p>
                <!--This should always be on 1 line!-->
                [envira-gallery-dynamic id="tags-EGI-{{annotationData.ctrl_null.expGroupId}}" columns="4" limit="4" sort="random"]
              </p>
            </div>
          {{/annotationData.ctrl_null}}
          <!-- !! Permissive WildType Strain + L4440-->

          <!--Permissive WildType Strain (N2) + RNAi ctrl_rnai-->
          {{#hasTaxTerm}}
            <div id="enviraEN">
              <h3>{{{screenType}}} {{{workflowData.temperature}}}C WS {{{ workflowData.biosamples.ctrlBiosample.name }}} + {{{libraryData.cosmid_id}}} {{{wellData.annotationData.taxTerm}}} RNAi</h3>
              <p>
                [envira-gallery-dynamic id="tags-EGI-{{{annotationData.ctrl_rnai.expGroupId}}}" columns="2"]
              </p>
            </div>
          {{/hasTaxTerm}}
          <!-- !! Permissive WildType Strain + RNAi-->

          <br><br>
        </div>
      </section>
    </div>
  </div>

</div>
